{
  "manifest_version": 3,
  "name": "Notes",
  "version": "1.1",
  "description": "This is a just a simple notes",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/16.png",
      "48": "img/48.png",
      "128": "img/128.png"
   }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
      "storage",
      "tabs"
  ],
  "icons": {
   "16": "img/16.png",
   "48": "img/48.png",
    "128": "img/128.png"
   }
}
